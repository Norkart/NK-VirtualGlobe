#X3D V3.0 utf8

PROFILE Interactive
COMPONENT xj3d_ParticleSystem:1
COMPONENT Rendering:4

Viewpoint {
  position 0 0 4
}

ParticleSystem {
  geometryType "POINT"
  maxParticles 200
  emitter PointEmitter {
    speed 0.1
    surfaceArea 0.0001
    mass 0.001
  }
  colorRamp ColorRGBA {
    color [
      1 1 1 1
      .5 .5 .5 1
      .3 .3 .3 1
      1 1 1 1
    ]
  }
  colorKey [ 0 3 4 5 ]

physics WindPhysicsModel {
    direction -1 0 0
    gustiness 1
    speed 0.1
    turbulence .2
  }
}

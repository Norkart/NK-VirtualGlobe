#VRML V2.0 utf8

NavigationInfo {
	type ["WALK"]
}

Transform {
	children [
		Shape {
			geometry IndexedFaceSet {
				coord Coordinate { 
					point [ -1 -1 0, 1 -1 0, 1 1 0, -1 1 0]
				}
				coordIndex [ 0 1 2 3]
				texCoord TextureCoordinate {
					point [0 0, 3 0, 3 3, 0 3]
				}
			}
			appearance Appearance {
				texture ImageTexture { url "vts.jpg" }
				material Material {}
				textureTransform TextureTransform {
					scale 2 2
					rotation 0.5
				}
			}
		}
	]
}

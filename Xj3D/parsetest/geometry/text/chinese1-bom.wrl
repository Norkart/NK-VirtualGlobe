#VRML V2.0 utf8
#Chinese text entered using MS Pinyin IME 3.0
#on Japanese Windows XP Pro by a person
#who does not speak Chinese.

Transform {
  translation 1 0 0
  children Shape {
    appearance Appearance {
      material Material {
        diffuseColor 1 0 0
      }
    }
    geometry Text {
      string [ "你好" ]
    }
  }
}

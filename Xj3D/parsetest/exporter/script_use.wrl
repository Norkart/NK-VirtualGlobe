#VRML V2.0 utf8

Switch {
   whichChoice -1
   children [
	  DEF SHAPE Shape {
	     geometry Box {}
	  }
   ]
}

DEF TRANSFORM Transform {
}

DEF SC Script {
  field SFNode shape USE SHAPE
  field SFNode transform USE TRANSFORM
  eventOut MFNode children

  url ["javascript:
     function initialize() {
        children = new MFNode(shape);
     }
  "]
}

ROUTE SC.children TO TRANSFORM.set_children


#VRML V2.0 utf8

EXTERNPROTO	EXP [
	eventIn	SFFloat	in
	field SFColor color
	field SFString marking
	eventOut SFInt32 out
] "complex_proto.wrl#myproto"

Transform {
	translation	0 3 0
	children [
DEF TESTER EXP	{
    marking "test"
	color 1 0 0
}
]}

Shape {
	geometry Sphere	{radius 0.25}
}

DEF	TIMER TimeSensor {
	loop TRUE
    cycleInterval 5
}

ROUTE TIMER.fraction_changed TO	TESTER.in

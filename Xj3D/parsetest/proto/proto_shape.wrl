#VRML V2.0 utf8

PROTO SPHERE [
   field SFFloat radius 1.0
   field SFColor color 1 1 1
]
{
   Shape {
      geometry Sphere { radius IS radius }
      appearance Appearance {
         material Material {
            diffuseColor IS color
         }
      }
   }
}

Group {
    children SPHERE { radius 0.5 color 1 0 0}
}
#VRML V2.0 utf8

PROTO RedApp [] {
  Appearance {
    material Material {
      diffuseColor 1 0 0
    }
  }
}
     
PROTO GreenApp [] {
  Appearance {
    material Material {
      diffuseColor 0 1 0
    }
  }
}

Transform {
  translation 0 1 0
  children Shape {
    appearance RedApp {}
    geometry Box {
    }
  }
}

Transform {
  translation 0 -1 0
  children Shape {
    appearance GreenApp {}
    geometry Box {
    }
  }
}



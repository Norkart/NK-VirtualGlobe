#VRML V2.0 utf8

PROTO MyBox [
  exposedField SFColor color 0 0 0
  field	SFVec3f	geomSize 2 2 2
] 
{
  Shape {
    appearance Appearance {
      material Material {
        diffuseColor IS color
      }
    }
    geometry Box {
		size IS	geomSize
    }
  }
}

MyBox { 
	color 0 1 0 
	geomSize 4 4 4
}

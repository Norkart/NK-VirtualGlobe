#VRML V2.0 utf8

PROTO TouchyBox [
]
{
	Group {
		children [
			Shape {
				appearance Appearance {
   					material Material { diffuseColor 1 0 0 }
  				}
				geometry Box {}
			}
			DEF TS TouchSensor {}

			DEF SC Script {
				eventIn SFTime touchInput

				url [ "EventTest.class" ]
			}

		]    
	}

	ROUTE TS.touchTime TO SC.touchInput
}

TouchyBox {}
